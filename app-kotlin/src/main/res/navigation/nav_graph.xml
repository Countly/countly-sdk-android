<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainListFragment">

    <fragment
        android:id="@+id/mainListFragment"
        android:name="ly.count.android.demo.kotlin.MainListFragment"
        android:label="@string/app_name">
        <action
            android:id="@+id/action_mainListFragment_to_secondListFragment"
            app:destination="@id/secondListFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondListFragment"
        android:name="ly.count.android.demo.kotlin.SecondListFragment"
        android:label="@string/new_button_title"
        tools:layout="@layout/fragment_second_list">
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
</navigation>